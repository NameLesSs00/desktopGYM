           <property name="styleSheet">
            <string notr="true">/* Main QDateEdit */
QDateEdit {
    border: 1px solid gray;       /* simple border */
    border-radius: 10px;          /* rounded corners */
    padding-left: 10px;           /* space for text */
    padding-right: 30px;          /* leave space for button */
}

/* Drop-down calendar button */
QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 25px;                  /* button width */
    border-left: none;
    border-radius: 10px;          /* rounded button like combo box */
    background: transparent;      /* no background color */
}

/* Arrow inside the button */
QDateEdit::down-arrow {
    image: url(:/C:/Users/b/Desktop/main/work/QT/public/downArrow.png);
    width: 12px;
    height: 12px;
}

/* Calendar popup */
QCalendarWidget {
    border: 1px solid gray;
    border-radius: 5px;           /* slightly rounded popup */
    padding: 5px;
    background-color: white;
}

/* Selected day */
QCalendarWidget QAbstractItemView::item:selected {
    background: lightgray;
}
